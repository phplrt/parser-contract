{
    "name": "phplrt/parser-contracts",
    "type": "library",
    "description": "A set of phplrt parser abstractions",
    "license": "MIT",
    "keywords": ["abstractions", "contracts", "decoupling", "interfaces", "interoperability", "standards", "parser", "syntax", "analysis", "language", "php"],
    "homepage": "https://github.com/phplrt",
    "support": {
        "issues": "https://github.com/phplrt/parser-contracts/issues",
        "source": "https://github.com/phplrt/parser-contracts"
    },
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "phplrt/ast-contracts": "^3.0",
        "phplrt/lexer-contracts": "^3.0",
        "phplrt/source-contracts": "^3.0",
        "phplrt/grammar-contracts": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Phplrt\\Contracts\\Parser\\": "src/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
